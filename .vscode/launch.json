{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run ghreleases2rss",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["--clearCategoryFeeds", "--file", "${workspaceFolder}/example_input_file.txt", "--category", "releasestmp"]
        },
        {
            "name": "Get ghreleases2rss version",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/main.go",
            "cwd": "${workspaceFolder}",
            "args": ["version"]
        }
    ]
}